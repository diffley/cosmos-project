# Development Environment Overrides - Managed by Ansible

# Enable local development mode
OPENC3_LOCAL_MODE=1

# Enable demo plugin for development  
OPENC3_DEMO=1

{% if cosmos_dev_registry is defined %}
# Development registry override
OPENC3_REGISTRY={{ cosmos_dev_registry }}
OPENC3_NAMESPACE={{ cosmos_dev_namespace | default('openc3inc') }}
{% endif %}

{% if cosmos_dev_tag is defined %}
# Development tag override
OPENC3_TAG={{ cosmos_dev_tag }}
{% endif %}